{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "jsx": "react-native",
    "outDir": "./build",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "baseUrl": "./",
    "paths": {
       
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/*/*.tsx",
    "src/*/*.ts",
   "src/screens/HomeScreen.jsx", "src/components/LogoIcon.jsx"  ],
  "exclude": [
    "node_modules",
    "build",
    "babel.config.js",
    "metro.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
